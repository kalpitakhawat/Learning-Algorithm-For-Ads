YUI.add("td-trending-mainview",function(e,t){"use strict";var n="model",r="config",i="container",s,o,u,a,f,l;e.namespace("TD.Trending").MainView=e.Base.create("TDTrendingMainView",e.Af.AppletView,[],{events:{".js-trending-history":{click:"onClickLoadHistory"}},initializer:function(){var t=this.get(n),u=t.get(r)||{},a=this,f,l;s=this.get(i),u.ui.horizontal_list&&(o=s.ancestor(".js-applet"),e.on("windowresize",a.updateTabState),e.on("domready",a.updateTabState)),s.one(".js-trending-ticker")&&(this.animationDuration=u.ui.animation_duration,this.animationWaitDuration=u.ui.animation_wait_duration,this.infiniteFIFO(this.animationDuration,this.animationWaitDuration),e.on("windowresize",e.bind(a.checkLinkLineIndex,this)),e.Global.on("header:mini",e.bind(a.resetTrending,this)),f=e.bind(a.cancelInfiniteFIFO,a),l=e.bind(a.infiniteFIFO,a,this.animationDuration,this.animationWaitDuration),s.one("ol").delegate("mouseenter",f,"a"),s.one("ol").delegate("mouseleave",l,"a"),e.Af.PageViz.on("visible",l,!1),e.Af.PageViz.on("hidden",f,!1)),u.giftsEnabled&&new e.Trending.trendingToggle(this)},updateTabState:function(){s.all("a").each(function(e){e.setAttribute("tabindex",e.inRegion(o)?"":"-1")})},onClickLoadHistory:function(t){var n={},r=this.get("container"),i=r.one(".js-trending-history");t.preventDefault(),r.one(".js-trending-history-summary").hasClass("D-n")?(n={rspns:"xpnd",t1:"a4",t2:"tc-ts",t3:"ct",sec:"tc-ts",elm:"itm",elmt:"xpnd",itc:"1"},e.Stencil.Toggle("add",".js-trending-history-summary",{"class":"js-trending-history-summary"}),e.Stencil.Toggle("add",".arrow-right",{"class":"Icon arrow-right D-n"}),e.Stencil.Toggle("add",".arrow-down",{"class":"Icon arrow-down"}),this.rapidBeaconClick(i,n)):(n={rspns:"clps",t1:"a4",t2:"tc-ts",t3:"ct",sec:"tc-ts",elm:"itm",elmt:"xpnd",itc:"1"},e.Stencil.Toggle("add",".js-trending-history-summary",{"class":"D-n js-trending-history-summary"}),e.Stencil.Toggle("add",".arrow-right",{"class":"Icon arrow-right"}),e.Stencil.Toggle("add",".arrow-down