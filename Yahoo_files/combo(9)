YUI.add("stencil-tabpanel",function(e){"use strict";function i(e){this.constructor.superclass.constructor.call(this,e)}var t="Selected",n="D-n",r="data-tabpanel-target";e.extend(i,e.Stencil.BasePlugin,{show:function(i,s){var o=this._node,u,a,f,l,c,h;return typeof s=="string"&&(s=o.one(s)),u=o.one("."+t),u===s?this:(a=e.one(u.getAttribute(r)||u.one("["+r+"]").getAttribute(r)),f=e.one(s.getAttribute(r)||s.one("["+r+"]").getAttribute(r)),this.processSources(f),a.removeClass(t).addClass(n),f.removeClass(n).addClass(t),h=s.get("tagName")==="OPTION",h&&(l=s.ancestor().get("children").indexOf(s),c=s.ancestor("select"),c.set("selectedIndex",l)),u.removeClass(t),s.addClass(t),this)},handleEvent:function(t){var n=t.currentTarget,r;n.get("tagName")==="SELECT"?r=n.get("options").item(n.get("selectedIndex")):r=t.target.ancestor("li",!0,"[data-plugin~=tabpanel]"),r&&(this.show(e.Stencil.getNodeList(this._node),r),t.preventDefault())}},{NS:"StencilTabPanel"}),e.Stencil.registerPlugin(i,"TabPanel")},"@VERSION@",{requires:["stencil-base","yui-base","dom-base","node-base","node-pluginhost"]});
