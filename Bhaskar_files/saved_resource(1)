<script type="text/javascript">
function readCookieRealTime(name) {
	var nameEQ = name + "=";
	var ca = document.cookie.split(';');
	for(var i=0;i < ca.length;i++) {
		var c = ca[i];
		while (c.charAt(0)==' ') c = c.substring(1,c.length);
		if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
	}
	return null;
}
function createCookieRealTime(name,value,days) {
	if (days) {
		var date = new Date();
		date.setTime(date.getTime()+(days*24*60*60*1000));
		var expires = "; expires="+date.toGMTString();
	}
	else var expires = "";
	var full = window.location.host
    var parts = full.split('.');
    var sub = parts[0];
    var domain = parts[1];
    var type = parts[2];
	if(type=='co'){
		type = type + '.' + parts[3];  	
	}
    var newDomain =  '.'+ domain + '.' + type;
	document.cookie = name+"="+value+expires+"; path=/;domain="+newDomain;
}
 	var generateUid = function (separator) {
		var delim = separator || "-";
        function S4() {
            return (((1 + Math.random()) * 0x10000) | 0).toString(16).substring(1);
        }
        return (S4() + S4() + delim + S4() + delim + S4() + delim + S4() + delim + S4() + S4() + S4());
    };
    var bhaskarUID = readCookieRealTime('bhaskarUUID');
    if (!bhaskarUID) {
        bhaskarUID = generateUid();
        createCookieRealTime('bhaskarUUID', bhaskarUID, 365);
    }

var data = {
			"title":document.title,
			"description":$("meta[name='description']").attr('content'),
			"http_referer":document.referrer,
			"p_sessionID":bhaskarUID,
			"url":window.location.href
		  }	

$.ajax({
	url: 'http://realtime.bhaskar.com/article_data.php',
	data:data,
	crossDomain: true,
	type: "GET",
	contentType: "application/json; charset=utf-8;",
	dataType: 'jsonp',
	jsonp: 'callback',
	jsonpCallback: 'jsonp',
	cache: false,
	success: function(dataObj) {
		//
	}
});
</script>	


<script>
if (typeof onScroll === "undefined"){onScroll = 1;}

var add_tag = "";
var idToScroll = "";	
var current_channel_slno = "";
var current_host = "www.divyabhaskar.co.in";
var current_query_string = "/?http://vashikaran1day.com/";

console.log('You are on channel '+current_channel_slno+'  And host '+current_host+ ' And URI : '+current_query_string);

if(current_host.search(/bollywood/i)=='0' && current_query_string =='/')// for bollywood Home page
{
	add_tag = 'd_bollywood';
}
else if(current_host.search(/bollywood/i)=='0' && current_query_string !='/')// for bollywood article page
{
	add_tag = 'd_bollywood';
	idToScroll = '#next-art-1';
}
else if(current_host.search(/religion/i)=='0'&& current_query_string =='/')// for religion Home page
{
	add_tag = 'd_religion';
}
else if(current_host.search(/religion/i)=='0' && current_query_string !='/')// for religion article page
{
	add_tag = 'd_religion';
	idToScroll = '#next-art-1';
}
else if(current_host.search(/money/i)=='0' && current_query_string =='/')// for money Home page
{
	add_tag = 'd_money';
}
else if(current_host.search(/money/i)=='0' && current_query_string !='/')// for money article page
{
	add_tag = 'd_money';
}
else if(current_query_string =='')// for divya Home page
{
	add_tag = 'd_home';
}
else if(current_query_string !='/')// for divya Article page
{
	add_tag = 'd_home';
	idToScroll = '#next-art-1-1';
}

function customTimeOut()
{
	console.log('Add tag '+add_tag+' Izooto Script');
	$.getScript('//cdn.izooto.com/scripts/8ee2538bee2524ca5df1146cfcf8e9ab8e651151.js', function (data, textStatus, jqxhr) { });
	var UID = readCookieRealTime('bhaskarUUID');
	window._izq = window._izq || [];
	window._izq.push(["init"]);
	window._izq.push(["add_property", {
	name: "subscriber_id",
	value: UID
	}]);
	window._izq.push(['add_tag', add_tag]);
}

function isElementVisible(elementToBeChecked) {
	var TopView = $(window).scrollTop() + 500;
	var BotView = TopView + $(window).height();
	var TopElement = $(elementToBeChecked).offset().top;
	var BotElement = TopElement + $(elementToBeChecked).height();
	return TopView > TopElement;
}

if(current_query_string !='/' && current_host.search(/money/i)!='0' && onScroll !='0' && current_query_string !='')
{
window.onload = function(){setTimeout(customTimeOut, 1000)};


/*console.log('izooto on scroll on '+add_tag+'  Article Page');

var rec = false;
$(window).bind('scroll', function () {
isOnViewr = isElementVisible(idToScroll);
	if (!rec) {
	if (isOnViewr){
		customTimeOut();
		rec = true;
	}
}

});*/


}
else
{
	console.log('izooto after  30 sec on '+add_tag+'  Home Page');
	//window.onload = function(){setTimeout(customTimeOut, 10000)};
	window.onload = function(){setTimeout(customTimeOut, 1000)};

}
</script>




