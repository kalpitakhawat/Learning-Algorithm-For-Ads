function clickEventTrack(id)
{
	var hiddenVal = $('#blocker_event_val_'+id).val();
	var strArr	  =  hiddenVal.split('::');

	var creative_id	= strArr[0];
	var href		= strArr[1];	
	var type		= strArr[2];	
	var event_type  = 'click';


	var data = "&widget_id=" + creative_id;
	data += "&action_type=" + event_type;
	data += "&type=" + type;
	var url = "http://realtime.bhaskar.com/article_widget_data.php?callback=parseRequest" + data;
	var script = document.createElement('script');
	script.setAttribute('src', url);
	document.getElementsByTagName('head')[0].appendChild(script);
	

	var popUp = window.open(href, '_blank' );       
	if (popUp === null || typeof popUp === "undefined") { 
	 location.href = href;               
	}else{                          
	 popUp.focus();                    
	}
	
}

