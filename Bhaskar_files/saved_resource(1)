<script>
jQuery("#flicker_eknazar").find('li').show();
if( changePager == 1){
flickerEknazarObjHome = $("#flicker_eknazar").bxSlider({displaySlideQty:1,moveSlideQty:1,auto:true,autoHover:true,autoControls:false,pager:false,pagerType:'short',speed: 1000,	pause: 5000,pagerLocation:'bottom',pagerShortSeparator:' of ',onSlideBefore: function($slideElement, oldIndex, newIndex){
	if(oldIndex == '1' && newIndex == '2')
	{
		$('.bx-pager').hide();
	}else
	{
		$('.bx-pager').show();
	}
	if( oldIndex == '2' && newIndex == '3' ){
		sleep(1000);
	}
	activeLazyImage($slideElement, oldIndex, newIndex,flickerEknazarObjHome);
	},
	onSlidePrev: function (){
	var slide_count = flickerEknazarObjHome.getSlideCount();
	var slide_curr = flickerEknazarObjHome.getCurrentSlide(); 
	var flag=0;

	//alert($("#checkSlide").val()+'==='+slide_curr);

	if(slide_curr!=$("#checkSlide").val()-1&& slide_curr>0 )
	{
		//console.log(slide_curr);
		slide_curr = $("#checkSlide").val()-1;
		//console.log(slide_curr);
	}
	//alert($("#checkSlide").val()+'=='+slide_curr);
	
	if($("#checkSlide").val()==0 && slide_curr==-1 ){
		//alert(slide_curr);
		slide_curr= 66;
			
	}
	$("#checkSlide").val(slide_curr);
	//alert(slide_curr);
	if(slide_curr==0)
	{
		slide_curr=1;
		flag=0;
	}
	else if(slide_curr<2)
	{
		slide_curr=slide_curr+1;
		flag=0;
	}else if(slide_curr==2)
	{
		//flag=1;
	}
	else
	{
		flag=0;
	}
		//slide_count=slide_count-1;
	if(flag==1){
		$("#pgValue").attr('style','display:block;line-height:20px;font-family:Arial, Helvetica, sans-serif;color:#cccccc !important ;font-size:11px;font-weight:normal;');
		$("#pgValue").text('Advertisement');
	}
	else
	{
		$("#pgValue").attr('style','display:block;');
		$("#pgValue").text(slide_curr+' of '+slide_count);
	} 
},
		onSlideNext: function (){
		var slide_count = flickerEknazarObjHome.getSlideCount();
		var slide_curr = flickerEknazarObjHome.getCurrentSlide();
		//alert(slide_curr);
		var flag=0;
		if(slide_curr==0)
		{
			slide_curr=0;
			flag=0;
		}
		else if(slide_curr<3)
		{
			//slide_curr=slide_curr+1;
			flag=0;
			if(slide_curr==3)
			{
				//flag=1;
			}
		}
		else
		{
			flag=0;
		}
		slide_curr=slide_curr+1;
		//slide_count=slide_count-1;
		if(flag==1){
			$("#pgValue").attr('style','display:block;line-height:20px;font-family:Arial, Helvetica, sans-serif;font-size:11px;color:#cccccc !important;font-weight:normal;');
			$("#pgValue").text('Advertisement');
		}
		else
		{
			$("#pgValue").attr('style','display:block;');
			$("#pgValue").text(slide_curr+' of '+slide_count);
		}
		$("#checkSlide").val(slide_curr);
		},
		onSliderLoad: function() {
		var slide_count = flickerEknazarObjHome.getSlideCount();
		//slide_count=slide_count-1;
			$("#pgValue").text('1 of '+slide_count);
		}
		});

}else{
var ipadpositionFlick = '';
//setTimeout(function(){
	jQuery("#flicker_eknazar").find('li').show();
	if(ipadpositionFlick){
		jQuery("#flicker_eknazar").bxSlider({displaySlideQty:1, moveSlideQty:1, auto:true, autoHover:true, autoControls:false, pager:true, pagerType:'short', pagerLocation:'bottom',pagerShortSeparator:' of '});
	}else{
		flickerbxSlider("#flicker_eknazar",true);
	}
}
