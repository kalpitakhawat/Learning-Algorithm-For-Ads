<script language="javascript" type="text/javascript">
//$(function(){ 
 var submitButton=$('.searchBtn');
 var inputBox=$('.searchInput');
 var searchBox=$('.searchForm');
	var isOpen = false;
	submitButton.click(function(){
		if(isOpen == false){
		searchBox.addClass('searchOpen');
		//inputBox.val('');
		//inputBox.focus();
		isOpen = true;
		 }else{
		   searchBox.removeClass('searchOpen');
		   inputBox.focusout();
		   isOpen = false;
			}
		});

		submitButton.mouseup(function(){return false;});
		searchBox.mouseup(function(){return false;});
		$(document).mouseup(function(){
		   if(isOpen == true){ 
			$('.searchBtn').css('display','block');
			submitButton.click();
		  }
	});

//});
function buttonUp(){
	var inputVal = $('.searchInput').val();
	inputVal = $.trim(inputVal).length;
		if(inputVal != 0){
			$('.searchBtn').css('display','none');
				}else{
				$('.searchInput').val('');
				$('.searchBtn').css('display','block');
				}
}
function submitSearchFooterform(thisobj){
    var  q =thisobj.q.value.replace(/\s+/g,"-");
        var action = thisobj.action;
        document.location = action+'?q='+q;
        return false;
}
</script>
