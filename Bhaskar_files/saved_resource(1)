<script type="text/javascript">
function showFlickImg(obj,flickIndex,type){ 
	
	var addpage = (type == 1)?1:1;
	var page_rows = 1;
	var length = jQuery(obj).children('li').length-2;
	flickIndex = (flickIndex)?flickIndex+addpage:addpage;
	//flickIndex = (flickIndex > rows)?rows:flickIndex;
	var rows = length;
	var pagenum = flickIndex; 
	if(!pagenum){
		pagenum = 1;
	}
	var last = Math.ceil(rows/page_rows);
	if (pagenum < 1) 
	{ 
		pagenum = 1; 
	} 
	else if (pagenum > last) 
	{ 
		pagenum = last; 
	}
	var start = (pagenum - 1) * page_rows;
	var end = start+page_rows;
	if(type == 1){
		end = end+5;
		end = (end > rows)?rows:end;
	}else{
		start = start-5;
		start = (start < 1)?0:start;	
	}
	for(var i = start; i<=end; i++){ 
			var imgObj = jQuery(obj).find('img.flkload').filter(":eq("+i+")");
			if(imgObj.attr('data-original')){imgObj.attr('src',imgObj.attr('data-original'));imgObj.removeAttr('data-original');}		
	}
}
function flickerbxSlider(flkID,autoType){
	autoType = (autoType)?true:false;
	jQuery(flkID).bxSlider({ displaySlideQty:1, moveSlideQty:1, auto:autoType, autoHover:true, autoControls:false, pager:true, pagerType:'short', pagerLocation:'bottom',pagerShortSeparator:' of ',
	onSlidePrev : function($slideElement, oldIndex, newIndex){showFlickImg(flkID,newIndex,2);},
	onSlideNext : function($slideElement, oldIndex, newIndex){showFlickImg(flkID,newIndex,1);}
});
}
function activeLazyImage(e,t,n,r){var i=r.getSlideCount();var s=t>n||t<=0&&n==i-1?"PREV":"NEXT";if(s=="PREV"){if(n==i-1){var o=e.find("img.lazyflk");var u=o.attr("data-original");o.attr("src",u).removeClass("lazyflk").removeAttr("data-original")}var o=e.prev().find("img.lazyflk")}else{if(n==1){var o=e.find("img.lazyflk");var u=o.attr("data-original");o.attr("src",u).removeClass("lazyflk").removeAttr("data-original")}var o=e.next().find("img.lazyflk")}var u=o.attr("data-original");o.attr("src",u).removeClass("lazyflk").removeAttr("data-original")}
</script>


