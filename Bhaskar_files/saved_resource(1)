<script type="text/javascript" > 
	var overTake=$("#div-gpt-ad-1418884107922-0_ad_container").html();	
	var time=new Date();	
	var timeChange=''
	timeChange=parseInt(timeChange);
	$.fn.nthParent = function(n){
	    var p = this;
	    for(var i=0;i<n;i++)
	        p = p.parent();
	    return p;
	}

	if (overTake != null) {
		if (!localStorage.getItem("takeovercount") && typeof(Storage)) {
			localStorage.setItem("takeovercount", 1);
			localStorage.setItem("isOn", "false");
		} else {
			var count = parseInt(localStorage.getItem('takeovercount')) + 1;
			localStorage.setItem('takeovercount', count);
			if (localStorage.getItem("isOn") == "true") {
				var count = parseInt(localStorage.getItem('takeovercount')) + 1;
				localStorage.setItem('takeovercount', count);
				localStorage.setItem("isOn", "false");
			}
		}
	}
	
	function redirectToSite(){			
			var bg=$("<div id='takeOverBg' style='position:absolute; left:0; top:0; background-color:#FFF; width:100%; z-index:999999; height:"+$(window).height()+"px;'></div>");
			//bg.prependTo($('#div-gpt-ad-1418884107922-0_ad_container').nthParent(4).find('body')[0]);	
			bg.prependTo($('body'));		
			var count=parseInt(localStorage.getItem('takeovercount'))+1;			
			localStorage.setItem("isOn","true");		
			window.location.href="/site-take-over-ads";	
	}
	
	if(parseInt(localStorage.getItem('takeovercount'))%2==0){
		
	}else{		
		(overTake!=null)?redirectToSite():"";
	}
</script>


