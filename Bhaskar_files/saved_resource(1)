// Slide Out Script - On click function //
$(".trigger").click(function(){
	$(".thumbs-panel").toggle("slow");
	$(this).toggleClass("active");
	return false;
});

function sleep(milliseconds) {
		var start = new Date().getTime();
			for (var i = 0; i < 1e7; i++) {
				if ((new Date().getTime() - start) > milliseconds){
				break;
				}
			}
		}

function makeVisible(pName) 
	{
	var imgObj = jQuery(".hoverimg_"+pName);
		if(imgObj.attr('data-original')){imgObj.attr('src',imgObj.attr('data-original'));imgObj.removeAttr('data-original');}	
		var item = document.getElementById(pName);
		var contentPanel = document.getElementById("flickerthumbs");
		var contents = contentPanel.getElementsByTagName("h3");
		for (var i = 0; i < contents.length; i++) {
		if (contents[i] != item) {
			contents[i].style.display = "none"
		}
	}
		item.style.display = "block";
		$("li").mouseout(function(){
			item.style.display = "none";
	});
}

