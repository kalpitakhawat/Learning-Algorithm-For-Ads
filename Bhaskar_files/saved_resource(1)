//For JA Impression Tracking in Wisdom
var trackClass = '';
var arrayDATA = [{'-sport':"JASPORT"},{'-nat':"JANATIONAL"},{'-state':"JASTATE"},{'-relg':"JARELIGION"},{'-bolly':"JABOLLOWOOD"},{'-int':"JAINTERNATIONAL"}]
	if(dhosts.indexOf('religion')!=-1){
	   add = '-relg';
	}else if(dhosts.indexOf('bollywood')!=-1){		  
       add = '-bolly';
	}
 for(var j=0;j<arrayDATA.length;j++){
    if(typeof arrayDATA[j][add]!='undefined'){
        trackClass = arrayDATA[j][add];
    }      
 }if(trackClass==''){	 
	 if(dhosts.indexOf('bhaskar.com')!=-1){		  
       trackClass = 'JAHOME';
	   //console.log(trackClass+'===============tracking class final');
	}
 }
 

var ff= false;
$(document).scroll(function () {
        var visible = isElementVisible("." + trackClass);
        if(!ff){ 
            if (visible) {				                  
                trackImpression(trackClass, 40);
                ff = true;
            }
        }
});


//Call if element is visible
function isElementVisible(elementToBeChecked) {
    var TopView = $(window).scrollTop() + 500;
    var BotView = TopView + $(window).height();
    var TopElement = $(elementToBeChecked).offset().top;
    var BotElement = TopElement + $(elementToBeChecked).height();
    return TopView > TopElement;
}
//Function For Tracking Impression For JA
function trackImpression(widget_name, position) { 
    var href = location.href;
    if (typeof widget_name != 'undefined' && widget_name != '') {
        var data = "position=" + position;
        data += "&widget_name=" + widget_name;
        data += "&hrefurl=" + href;
        var url = "http://realtime.bhaskar.com/tracking_imp.php?callback=parseRequest&" + data;
        var script = document.createElement('script');
        script.setAttribute('src', url);
        document.getElementsByTagName('head')[0].appendChild(script);
    }
}
window.addEventListener( "message",function (e) {
  if(e.origin.search("ultima") < 0){ return; }      
	 var resArr 	= decodeURIComponent(e.data).split("-::-");
	 if(resArr[0]=='gaevent'){
		var myobj = JSON.parse(resArr[1]);
		dataLayer.push(myobj.gaevent);
}
},false);
</script>

