<script type="text/javascript">
function isIE () {
  var myNav = navigator.userAgent.toLowerCase();
  return (myNav.indexOf('msie') != -1) ? parseInt(myNav.split('msie')[1]) : false;
}
function openVideoPopUp(id)
{
	var fsrc = $("#current_video").attr('src');
	if(fsrc !='' && fsrc!=undefined){
		var ifrmSrc=$('#current_video').attr('src').split("?")[0];
		var newSrc=ifrmSrc+"?autoplayFlag=false&amp;mode=html";
		$("#current_video").attr("src",newSrc);
	}
	openMyModal('/dbtv_video_detail/'+id+'/', 660, 360);return false;
}

jQuery('#flicker_dbtv').bxSlider({minSlides: 3,maxSlides: 3, slideMargin: 0,speed :1000,  pager:false,  pagerType:'short', pagerLocation:'bottom',pagerShortSeparator:' of ', autoControls:false, auto:true,infiniteLoop: true, autoHover:true,hideControlOnEnd:true, moveSlides:3});
$(document).ready(function() {
	$('.dbtv-home-nav ul li a').click(function(){
		$('.dbtv-home-nav ul li').removeClass('c_color');
		$(this).parent().addClass('c_color');
		var catId = $(this).attr('id');
		var strHtml	=	"";
		if (isIE () && isIE () < 10) {	
		var xdr = new XDomainRequest();
		$("#ajaxloaderdbtv").addClass("ajaxloaderdbtv");
		xdr.open("get",'http://www.bhaskar.com/video-you-may-like/?limit=0,12&track=videos-hws&wid=216&hei=187&homepagevideo=1&chid=960&catid='+catId);
		xdr.onload = function () {
		var JSON = $.parseJSON(xdr.responseText);
		if (JSON == null || typeof (JSON) == 'undefined'){
			JSON = $.parseJSON(data.firstChild.textContent);
		}
		var data = JSON;
		strHtml +='<ul id="flicker_dbtv">';
		$.each(data, function(i, row) {
			strHtml +='<li><p><a href="'+row.url+'" rel="nofollow" title="'+row.title+'">'+row.title+'</a></p><div class="flicker-imgbox"> <a href="'+row.url+'" rel="nofollow" title="'+row.title+'" class="imgbox"> <img class="flikrlazy" src="'+row.img+'" alt="'+row.title+'" title="'+row.title+'" width="216px" height="187px" /><div class="homedb-play"></div></a> </div></li>';		
		});
		$("#flicker_dbtv").remove();
		$("#ultimadbtv").html(strHtml);
		$("#ajaxloaderdbtv").removeClass("ajaxloaderdbtv");
		jQuery('#flicker_dbtv').bxSlider({minSlides: 3,maxSlides: 3, slideMargin: 0,speed :1000,  pager:false,  pagerType:'short', pagerLocation:'bottom',pagerShortSeparator:' of ', autoControls:false, auto:true,infiniteLoop: true, autoHover:true,hideControlOnEnd:true, moveSlides:3});
    };
    xdr.onprogress = function(){ };
    xdr.ontimeout = function(){ $("#ajaxloaderdbtv").removeClass("ajaxloaderdbtv"); };
    xdr.onerror = function () { $("#ajaxloaderdbtv").removeClass("ajaxloaderdbtv");};
    setTimeout(function(){
        xdr.send();
    }, 0);
	
}else{
	$("#ajaxloaderdbtv").addClass("ajaxloaderdbtv");
	$.get('http://www.bhaskar.com/video-you-may-like/?limit=0,12&track=videos-hws&wid=216&hei=187&homepagevideo=1&chid=960&catid='+catId,function(data) {		
		strHtml +='<ul id="flicker_dbtv">';
		$.each(data, function(i, row) {
		strHtml +='<li style="float: left; position: relative"><p><a href="'+row.url+'" rel="nofollow" title="'+row.title+'">'+row.title+'</a></p><div class="flicker-imgbox"> <a href="'+row.url+'" title="'+row.title+'" class="imgbox"> <img class="flikrlazy" src="'+row.img+'" alt="'+row.title+'" title="'+row.title+'" width="216px" height="187px" /><div class="homedb-play"></div></a> </div></li>';
		});
		strHtml +='</ul>';
		$("#flicker_dbtv").remove();		
		$("#ultimadbtv").html(strHtml);
		$("#ajaxloaderdbtv").removeClass("ajaxloaderdbtv");
		jQuery('#flicker_dbtv').bxSlider({minSlides: 3,maxSlides: 3, slideMargin: 0,speed :1000,  pager:false,  pagerType:'short', pagerLocation:'bottom',pagerShortSeparator:' of ', autoControls:false, auto:true,infiniteLoop: true, autoHover:true,hideControlOnEnd:true, moveSlides:3});
	
		
},'json').fail(function() {
   $("#ajaxloaderdbtv").removeClass("ajaxloaderdbtv");
  });

}


	});
});
</script> 

