     <script type="text/javascript">
		 function ScriptPath()
		 {
			 var dName="";
			 var dimainName=window.location.hostname;		 		 
			 if(dimainName.indexOf('bollywood')!= -1 && dimainName.indexOf('bhaskar.com')!= -1){ dName="Bollywood Bhaskar.com";}else if(dimainName.indexOf('religion')!= -1 && dimainName.indexOf('bhaskar.com')!= -1){ dName="Religion Bhaskar.com";}else if(dimainName.indexOf('gallery')!= -1 && dimainName.indexOf('bhaskar.com')!= -1){ dName="Gallery Bhaskar.com";}else if(dimainName.indexOf('daily')!= -1){ dName="Daily Bhaskar.com";}else if(dimainName.indexOf('divyamarathi')!= -1){ dName="Divyamarathi Bhaskar.com";}else if(dimainName.indexOf('fashion101')!= -1){ dName=document.domain;}else if(dimainName.indexOf('divyabhaskar')!= -1){ dName=document.domain;}else{ dName="Bhaskar.com" }
			
			var path=window.location.href; 
			var uriArr = path.split("/"); 
			var returnStr	=	dName+" : ";
			if(uriArr[3] && uriArr[3].indexOf('?')==-1){
			  returnStr	+=	uriArr[3];	
			}else{
			returnStr	+=	'Home Page';
			}
			if(uriArr[4] && uriArr[4].indexOf('?')==-1){
			returnStr	+=	" : "+uriArr[4];	
			}
			if(uriArr[5] && uriArr[5].indexOf('?')==-1){
			returnStr	+=	" : "+uriArr[5];	
			}
			
			if(uriArr[6] && uriArr[6].indexOf('?')==-1){
			returnStr	+=	" : "+uriArr[6];	
			}
			
			return returnStr;
		}
	</script>	
	


