	$(document).ready(function(){
		var  flag=1;
		var  checkValue=0;	
		var pusherObj=$("#div-gpt-ad-1379583618720-0_ad_container").html();	
	    if(typeof pusherObj=="undefined" || pusherObj==null){
		elementOffset = $('.mainnav').offset().top;
		$(window).scroll(function() {
			var e = $(window).scrollTop();			
			distance      = (elementOffset - e);		  
		 if (e >=elementOffset) {
			$(".mainnav").addClass("divbhassticky");
			$(".mainnav ul li.home").addClass("gridIcon");
			//$(".mainnav ul li.gridIcon a").attr({title: "Menu"}); 
			$(".mainnav ul li.gridIcon a.hicons").attr({title: "Menu", href:"javascript:void(0)"});	
			} else {
			$(".mainnav").removeClass("divbhassticky");
			$(".mainnav ul li.gridIcon a").attr({title: "Home", href:"http://www.divyabhaskar.co.in"});
			$(".mainnav ul li.home").removeClass("gridIcon");
			$(".stickyhover").hide();
			$('.home').removeClass('nHover')
		   }
		
	   });
	 }
	
	
	});
	
	 $(function(){
		var hover=false;
		
		$('.lhs-main-nav').delegate('li.gridIcon','click',function(event){ 
		event.stopPropagation();
			$('.stickyhover').slideToggle() 
			$( this ).toggleClass( "nHover" );
		});
		
		$('.stickyhover').click(function(event){
		event.stopPropagation();
		});
		
			
		$("html").click(function() {
		$('.stickyhover').stop(true,true).slideUp('fast')
		$('.home').removeClass('nHover')
			});	
		
		
		});
	 
	
