<script type="text/javascript">

function overEffect(curLi){
	
	if(curLi==1){
		$("#li1").addClass("current");
		$("#li2").removeClass("current");
		$("#li3").removeClass("current");
		$("#1-tab").attr('style','display:block');
		$("#2-tab").attr('style','display:none');
		$("#3-tab").attr('style','display:none');
	}else if(curLi==2){		
		$("#li1").removeClass("current");
		$("#li2").addClass("current");
		$("#li3").removeClass("current");
		$("#1-tab").attr('style','display:none');
		$("#2-tab").attr('style','display:block');
		$("#3-tab").attr('style','display:none');
	}else if(curLi==3){		
		$("#li1").removeClass("current");
		$("#li2").removeClass("current");
		$("#li3").addClass("current");
		$("#1-tab").attr('style','display:none');
		$("#2-tab").attr('style','display:none');
		$("#3-tab").attr('style','display:block');
	}

}
</script>

