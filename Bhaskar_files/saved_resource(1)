//@website: www.bhaskar.com				
$(document).ready(function(){
	$("div.rashifal").css("display", "block");
	$(".menu > li").click(function(e){
		switch(e.target.id){
			case "rashifal":
			 
				//change status & style menu
				$("#rashifal").addClass("active");
				$("#subhmuhrat").removeClass("active");
				$("#selfhelp").removeClass("active");
				
				//display selected division, hide others
				$("div.rashifal").fadeIn();
				$("div.subhmuhrat").css("display", "none");
				$("div.selfhelp").css("display", "none");
				
			break;
			case "subhmuhrat":
			 
				//change status & style menu
				$("#rashifal").removeClass("active");
				$("#subhmuhrat").addClass("active");
				$("#selfhelp").removeClass("active");
				
				//display selected division, hide others
				$("div.subhmuhrat").fadeIn();
				$("div.rashifal").css("display", "none");
				$("div.selfhelp").css("display", "none");
				
			break;			
			case "selfhelp":
				//change status & style menu
				$("#rashifal").removeClass("active");
				$("#subhmuhrat").removeClass("active");
				$("#selfhelp").addClass("active");
				//display selected division, hide others
				$("div.selfhelp").fadeIn();
				$("div.rashifal").css("display", "none");
				$("div.subhmuhrat").css("display", "none");
				break;			
		}
		//alert(e.target.id);
		return false;
	});
});

