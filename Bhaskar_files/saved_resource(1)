<script type="text/javascript">
var setflagnew200 = 1;
   function ent_im_sflick() {
       var e = window.pageYOffset || document.body.scrollTop || document.documentElement.scrollTop;
       var t = parseInt($(window).height());
       var r = parseInt($("#money-bhaskar-container").position().top) - t;
       if (e > r && setflagnew200 == 1) {
           $("#moneybhaskar_flicker").find("li").show();	
moneyBhaskarFliker = $("#moneybhaskar_flicker").bxSlider({displaySlideQty:1,speed:1000, moveSlideQty:1, auto:true, autoHover:true, autoControls:false, pager:true, pagerType:'short', pagerLocation:'bottom',pagerShortSeparator:' of ',
			onSlideBefore: function($slideElement, oldIndex, newIndex){
				activeLazyImage($slideElement, oldIndex, newIndex,moneyBhaskarFliker);
			}
});
  setflagnew200++;
       }
   }


   function addEvent_sflick(obj, type, fn) {
       if (obj.addEventListener) {
           obj.addEventListener(type, fn, false);
       } else if (obj.attachEvent) {
           obj["e" + type + fn] = fn;
           obj[type + fn] = function() {
               obj["e" + type + fn](window.event);
           }
           obj.attachEvent("on" + type, obj[type + fn]);
       }
   }

addEvent_sflick(window, 'scroll', ent_im_sflick);



</script>  </div>
