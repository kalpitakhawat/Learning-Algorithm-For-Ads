<script>
	var setflagnew100=1;
	function ent_im_bflick(){
		var e = window.pageYOffset||document.body.scrollTop||document.documentElement.scrollTop;
		var t=parseInt($(window).height());
		var r=parseInt($("#position_bollywood").position().top)-t;
		if(e>r && setflagnew100==1){
			$("#bollywood_flicker").find("li").show();
			bollyFlickerObj = $("#bollywood_flicker").bxSlider({ displaySlideQty:1, moveSlideQty:1,speed :1000, auto:true, autoHover:true,pagerType:'short',pagerShortSeparator:' of ',onSlideBefore:function($slideElement, oldIndex, newIndex){activeLazyImage($slideElement, oldIndex, newIndex,bollyFlickerObj);$(window).trigger("scroll");}});
			setflagnew100++;
		}
	}
	function addEvent_bflick( obj, type, fn ){ 
		if (obj.addEventListener){ 
			obj.addEventListener( type, fn, false );
		}else if (obj.attachEvent){ 
			obj["e"+type+fn] = fn; 
			obj[type+fn] = function(){ obj["e"+type+fn]( window.event ); } 
			obj.attachEvent( "on"+type, obj[type+fn] ); 
		}
	}
	addEvent_bflick(window, 'scroll', ent_im_bflick);
</script>

