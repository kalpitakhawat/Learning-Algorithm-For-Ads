<script type="text/javascript">
var flagWidget=1;
$(window).scroll(function(){
if($('#topnewsWidget_1').length >0 && flagWidget==1){
	var topnewsWidget = $("#topnewsWidget_1").offset().top;
	var tnw = $(window).scrollTop();
	if (tnw > parseInt(topnewsWidget)-1800) {
		flagWidget++;
		$('#topnewsWidget_1').html('<iframe src="http://www.bhaskar.com/top-news-widget-3.html" height="400" width="323" scrolling="no" frameborder="0"></iframe>');
		$('#topnewsWidget_2').html('<iframe src="http://daily.bhaskar.com/top-news-widget-3.html" height="400" width="323" scrolling="no" frameborder="0"></iframe>');
		$('#topnewsWidget_3').html('<iframe src="http://divyamarathi.bhaskar.com/top-news-widget-3.html" height="400" width="300" scrolling="no" frameborder="0"></iframe>');
	}
}
});
</script>
