/* ******** Realtime Tracking JS function  START */
function wistrk(storyid,mmdv,seq,wid,obj)
{
	var widgetsid=$('#widgetsid').val();
if(widgetsid != null && widgetsid != undefined){
		wid=widgetsid;
		}else{
		wid=wid;
		}
var title=encodeURIComponent(obj.title);
	var data = "storyid="+storyid;
	data += "&mmdv="+mmdv;
	data += "&seq="+seq;
	data += "&wid="+wid;
	data += "&title="+title;
	data += "&url="+obj.href;
	var url = "http://realtime.bhaskar.com/widget_data.php?callback=wistrk&"+data;
	var script = document.createElement('script');
	script.setAttribute('src', url);
	document.getElementsByTagName('head')[0].appendChild(script);
}
/* ******** Realtime Tracking JS function END */